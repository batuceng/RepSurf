ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda-11.3/bin/nvcc

cflags = -DTORCH_EXTENSION_NAME=pointops_cuda -DTORCH_API_INCLUDE_EXTENSION_H -isystem /opt/anaconda3/lib/python3.6/site-packages/torch/include -isystem /opt/anaconda3/lib/python3.6/site-packages/torch/include/torch/csrc/api/include -isystem /opt/anaconda3/lib/python3.6/site-packages/torch/include/TH -isystem /opt/anaconda3/lib/python3.6/site-packages/torch/include/THC -isystem /usr/local/cuda-11.3/include -isystem /opt/anaconda3/include/python3.6m -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++14
post_cflags = 
cuda_cflags = -DTORCH_EXTENSION_NAME=pointops_cuda -DTORCH_API_INCLUDE_EXTENSION_H -isystem /opt/anaconda3/lib/python3.6/site-packages/torch/include -isystem /opt/anaconda3/lib/python3.6/site-packages/torch/include/torch/csrc/api/include -isystem /opt/anaconda3/lib/python3.6/site-packages/torch/include/TH -isystem /opt/anaconda3/lib/python3.6/site-packages/torch/include/THC -isystem /usr/local/cuda-11.3/include -isystem /opt/anaconda3/include/python3.6m -D_GLIBCXX_USE_CXX11_ABI=0 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_75,code=sm_75 --compiler-options '-fPIC' -std=c++14
cuda_post_cflags = 
ldflags = -shared -L/opt/anaconda3/lib/python3.6/site-packages/torch/lib -lc10 -lc10_cuda -ltorch_cpu -ltorch_cuda -ltorch -ltorch_python -L/usr/local/cuda-11.3/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  command = $nvcc $cuda_cflags -c $in -o $out $cuda_post_cflags

rule link
  command = $cxx $in $ldflags -o $out

